#include <stdio.h>

// Defines PI macro
#define PI 3.1415

int main()
{
 
 // Get input for radius
 double radius;
 scanf("%lf", &radius);
 
 // Calculates area of circle 
 double area = PI * (radius * radius);
 
 // Prints area of circle up to 2 decimal places
 printf("Area = %.2lf", area);
 
 return 0;
}
